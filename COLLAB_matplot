
import matplotlib.pyplot as plt
s={"E":10,"A":9,"B":8,"C":7,"D":6,"P":5}
c=list(s.keys())
p=list(s.values())
print(c)
print(p)
fig=plt.figure(figsize=(9,9))
plt.pie(x=p ,labels= c,
        explode=[0.15,0.03,0.04,0.05,0.07,0.08],
        shadow= "true",
        
        )       
plt.title("Student grade",size=40,fontweight="bold",color="red")
plt.legend(["excellent","very good","good","average","okay","need to improve"])
